    (function () {
        if (!isDomainSafe()){
            //обращение через [] обязательно для обхода зеркала http://flawedgate.appspot.com/kontur.ru
            window.location['href'] = "https://kontur.ru";
        }

        function isDomainSafe(){
            var hostname = window.location.hostname;
            var availableDomains = ["kontur.ru","buhonline.ru","kontur-extern.ru","diadoc.ru","konturclick.ru"];

            if (availableDomains.length == 0) {
                return true;
            }

            for (var i = 0; i < availableDomains.length; i++) {
                var domain = availableDomains[i];
                var inSafeDomainRegExp = new RegExp("(^|.*\.)" + regExpEscape(domain) + "$", "i");

                if (inSafeDomainRegExp.test(hostname)) {
                    return true;
                }
            }

            return false;
        }

        function regExpEscape(s) {
            return s.replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&');
        }
    })();
</script>

